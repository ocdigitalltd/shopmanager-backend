{
  "name": "nodejs-parsing-tool",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "dev": "cross-env NODE_ENV=development NODE_PATH=. nodemon --config nodemon.json src/app.ts",
    "prod": "tsc && cross-env NODE_ENV=production NODE_PATH=. node dist/app.js",
    "pgDatabaseReset": "npm run pgDatabaseRollback && npm run pgDatabaseMigrations && npm run pgDatabaseSeeds",
    "pgDatabaseMkTable": "knex migrate:make $TABLE",
    "pgDatabaseRollback": "cross-env NODE_ENV=development NODE_PATH=. knex migrate:rollback",
    "pgDatabaseMigrations": "cross-env NODE_ENV=development NODE_PATH=. knex migrate:latest",
    "pgDatabaseSeeds": "cross-env NODE_ENV=development NODE_PATH=. knex seed:run",
    "prodMigrations": "cross-env NODE_ENV=production NODE_PATH=. knex migrate:latest",
    "prodSeeds": "cross-env NODE_ENV=production NODE_PATH=. knex seed:run"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/preset-typescript": "7.18.6",
    "@types/express": "4.17.14",
    "@types/express-session": "1.17.5",
    "@types/node": "18.7.21",
    "@types/node-fetch": "2.6.2",
    "axios": "^1.7.4",
    "body-parser": "1.20.0",
    "cross-env": "7.0.3",
    "express": "4.18.1",
    "express-session": "1.17.3",
    "googleapis": "^126.0.1",
    "knex": "0.21.15",
    "libphonenumber-js": "^1.10.45",
    "moment": "^2.29.4",
    "node-cron": "^3.0.2",
    "node-html-parser": "^6.1.10",
    "nodemailer": "^6.9.7",
    "ovh": "^2.0.3",
    "pg": "8.8.0",
    "postgres": "3.2.4",
    "puppeteer": "^21.3.6",
    "ts-node": "10.9.1",
    "sql-formatter": "^15.0.2",
    "@types/nodemailer": "^6.4.14",
    "nodemon": "2.0.20", 
    "typescript": "4.8.3"
  },
  "devDependencies": {
    "@types/express": "4.17.14",
    "@types/express-session": "1.17.5",
    "@types/node": "18.7.21",
    "@types/node-fetch": "2.6.2",
    "@types/nodemailer": "^6.4.14",
    "nodemon": "2.0.20", 
    "ts-node": "10.9.1",
    "typescript": "4.8.3"
  }
}
